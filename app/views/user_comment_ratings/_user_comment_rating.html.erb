<div id="<%= dom_id user_comment_rating %>" class="user-comment-rating">
  <p class="rating-value">
    <strong>Value:</strong>
    <%= user_comment_rating.value %>
  </p>

  <p class="comment-id">
    <strong>Comment:</strong>
    <%= user_comment_rating.comment_id %>
  </p>

  <p class="user-id">
    <strong>User:</strong>
    <%= user_comment_rating.user_id %>
  </p>
</div>
