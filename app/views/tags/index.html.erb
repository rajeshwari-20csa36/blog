<p class="notice" style="color: green"><%= notice %></p>

<h1 class="tags-title">Tags</h1>

<div id="tags">
  <% @tags.each do |tag| %>
    <%= render tag %>
    <p>
      <%= link_to "Show this tag", tag, class: "show-tag-link" %>
    </p>
  <% end %>
</div>

<%= link_to "New tag", new_tag_path, class: "new-tag-link" %>
